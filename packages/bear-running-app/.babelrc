{
  "presets": ["flow"],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "~": "./src",
          "types": "../types",
          "api": "./api-build"
        }
      }
    ],
    ["transform-react-jsx", { "pragma": "h" }],
    "transform-class-properties",
    "transform-es2015-modules-commonjs",
    "transform-object-rest-spread",
    "styled-jsx/babel"
  ]
}
