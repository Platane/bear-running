{
  "presets": ["flow"],
  "plugins": [
    "emotion",
    [
      "module-resolver",
      {
        "alias": {
          "~": "./src",
          "types": "../types",
          "api": "./api-build"
        }
      }
    ],
    ["transform-react-jsx", { "pragma": "h" }],
    "transform-class-properties",
    "transform-es2015-modules-commonjs",
    "transform-object-rest-spread",
    "styled-jsx/babel"
  ],
  "env": {
    "production": {
      "plugins": [
        "emotion",
        [
          "module-resolver",
          {
            "alias": {
              "~": "./src",
              "types": "../types"
            }
          }
        ],
        ["transform-react-jsx", { "pragma": "h" }],
        "transform-class-properties",
        "transform-es2015-modules-commonjs",
        "transform-object-rest-spread",
        "styled-jsx/babel",
        [
          "transform-runtime",
          {
            "helpers": false,
            "polyfill": false,
            "regenerator": true,
            "moduleName": "babel-runtime"
          }
        ]
      ],
      "presets": [
        "flow",
        [
          "env",
          {
            "targets": {
              "browsers": ["last 2 Chrome versions", "last 2 Firefox versions"]
            }
          }
        ]
      ]
    }
  }
}
